@model RegisterUserCollageIdModel


@{
    ViewBag.title = "User Collage Register";
}

<div class="row">
    <div class="col-md-12">
        <form method="post">
            <div asp-validation-summary="All" class="text-danger"></div>



            <div class="form-group">
                @Html.LabelFor(model => model.collageId, new { @class = "control-label col-md-2" })
                @Html.DropDownList(
                "collageId",
                ViewBag.selectCollageList,
                "Select Collage",
                new { @class = "form-control" })
            </div>
           

            <br/>
            @Html.TextBox("studentId")


            <br />

            <button type="submit" class="btn btn-primary">
                Register
            </button>
        </form>

    </div>
